<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://ed-fi.org/5.2.0" xmlns:ann="http://ed-fi.org/annotation" targetNamespace="http://ed-fi.org/5.2.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:include schemaLocation="Ed-Fi-Core.xsd" />
  <xs:annotation>
    <xs:documentation>===== Ed-Fi 5.2.0 Extensions =====</xs:documentation>
  </xs:annotation>
  <xs:annotation>
    <xs:documentation>===== Domain Entities =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-AssessmentAccommodation">
    <xs:annotation>
      <xs:documentation>This entity represents the variations used in how an assessment is presented or taken.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="AssessmentReference" type="AssessmentReferenceType">
            <xs:annotation>
              <xs:documentation>The assessment for which the accommodations are available.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Accommodation" type="AccommodationDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>This descriptor defines variations used in how an assessment is presented or taken.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>AccommodationDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CalendarDateExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="CalendarDate">
        <xs:sequence>
          <xs:element name="EventMinutes" type="EXTENSION-EventMinutes" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The amount of time for the event in minutes as recognized by the school.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EventDuration" type="EventDuration" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The amount of time for the event as recognized by the school: 1 day = 1, 1/2 day = 0.5, 1/3 day = 0.33.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CalendarExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Calendar">
        <xs:sequence>
          <xs:element name="InstructionalTimeInMinutesIndicator" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>Indicator of whether or not a school is calculating instructional time in minutes instead of days.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-DisciplineActionExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DisciplineAction">
        <xs:sequence>
          <xs:element name="InterimAlternativeEducationSetting" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicator to denote if the special education student was referred to an interim alternative education setting.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DisciplineActionService" type="EXTENSION-DisciplineActionServiceDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The services provided for the student during a discipline action.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-DisciplineActionServiceDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentArrest" type="EXTENSION-StudentArrestDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The arrest type for a discipline action.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-StudentArrestDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="ArrestReason" type="EXTENSION-ArrestReasonDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The arrest reason codes for a discipline action.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-ArrestReasonDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-EducationOrganizationOtherPersonnel">
    <xs:annotation>
      <xs:documentation>Noncertified and other personnel employed by the School Corporation, organized by category (e.g., Office, Clerical, Cafeteria, Maintenance).</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="LocalEducationAgencyReference" type="LocalEducationAgencyReferenceType">
            <xs:annotation>
              <xs:documentation>Reference to the LEA that employees the personnel.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="OtherPersonnelCategory" type="EXTENSION-OtherPersonnelCategoryDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>Category for noncertified and other personnel.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-OtherPersonnelCategoryDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="OtherPersonnelTitle" type="EXTENSION-OtherPersonnelTitleDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>Position title for noncertified and other personnel.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-OtherPersonnelTitleDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="OtherPersonnelCount" type="EXTENSION-OtherPersonnelCount" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Total number of noncertified and other personnel employed by the school corporation by position title.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FullTimeEquivalency" type="EXTENSION-FullTimeEquivalency" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The ratio between the hours of work expected in a position and the hours of work normally expected in a full-time position in the same setting.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-LocalAccountExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="LocalAccount">
        <xs:sequence>
          <xs:element name="SectionSectionDimensionReference" type="EXTENSION-SectionDimensionReferenceType">
            <xs:annotation>
              <xs:documentation>References the section dimension with which the local account is associated.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FunctionFunctionDimensionReference" type="FunctionDimensionReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>References the function dimension with which the local account is associated.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FundFundDimensionReference" type="FundDimensionReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>References the fund dimension with which the local account is associated.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ObjectObjectDimensionReference" type="ObjectDimensionReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>References the object dimension with which the local account is associated.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="OperationalUnitOperationalUnitDimensionReference" type="OperationalUnitDimensionReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>References the operational unit dimension with which the local account is associated.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SubCategorySubCategoryDimensionReference" type="EXTENSION-SubCategoryDimensionReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>References the subcategory dimension with which the local account is associated.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-LocalCapitalizedEquipment">
    <xs:annotation>
      <xs:documentation>The set of local education agency or charter management organization expense or revenue amounts.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="LocalAccountReference" type="LocalAccountReferenceType">
            <xs:annotation>
              <xs:documentation>References the local account with which the local capitalized equipment is associated.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AsOfDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The date of the reported amount for the account.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="RecordIdentifier" type="EXTENSION-RecordIdentifier">
            <xs:annotation>
              <xs:documentation>A unique number or alphanumeric code that identifies the Record.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FinancialCollection" type="FinancialCollectionDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>The accounting period or grouping for which the amount is collected.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>FinancialCollectionDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="EquipmentType" type="EXTENSION-EquipmentType">
            <xs:annotation>
              <xs:documentation>An equipment that qualify for capitalization, encompassing both nontechnology-related and technology-related equipment. Nontechnology-related equipment  can include items like furniture and other assets with a capital nature. Technology-related equipment can inlcude technology hardware and technology infrastructure.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EquipmentDescription" type="EXTENSION-EquipmentDescription" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The description of the equipment/vehicle that was purchased.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AcquisitionDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The date when the equipment was purchased.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PaymentAmount" type="Currency">
            <xs:annotation>
              <xs:documentation>The amount paid for the equipment associated with the associated expenditure information.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PerUnitCost" type="Currency">
            <xs:annotation>
              <xs:documentation>The unit cost of the equipment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CapitalizedThreshold" type="Currency">
            <xs:annotation>
              <xs:documentation>The lesser amount of the captilized threshold or $5,000, according to the school corporation's captilization threshold policy.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-LocalSubaward">
    <xs:annotation>
      <xs:documentation>Subawards: An award provided by a pass-through entity to a subrecipient for the subrecipient to carry out part of a Federal award received by the pass-through entity.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="LocalAccountReference" type="LocalAccountReferenceType">
            <xs:annotation>
              <xs:documentation>References the local account with which the local subward is associated.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AsOfDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The date of the reported amount for the account.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="RecordIdentifier" type="EXTENSION-RecordIdentifier">
            <xs:annotation>
              <xs:documentation>A unique number or alphanumeric code that identifies the Record.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FinancialCollection" type="FinancialCollectionDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>The accounting period or grouping for which the amount is collected.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>FinancialCollectionDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="VendorOrganizationName" type="EXTENSION-VendorOrganizationName">
            <xs:annotation>
              <xs:documentation>The name of the vendor or organization who is engaged in the contract with the LEA.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DepartmentName" type="Department">
            <xs:annotation>
              <xs:documentation>The name of the department associated with the subcontact/subaward.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ContractNumberOfYears" type="EXTENSION-ContractNumberOfYears">
            <xs:annotation>
              <xs:documentation>This number reflects if the recording fiscal year wa the first, second, etc. Year of the contract.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SubawardAmount" type="Currency">
            <xs:annotation>
              <xs:documentation>Total award provided by a pass-through entity to a subrecipient for the subrecipient to carry out part of a Federal award received by the pass-through entity.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ExpenditureAmount" type="Currency">
            <xs:annotation>
              <xs:documentation>An award provided by a pass-through entity to a subrecipient for the subrecipient to carry out part of a Federal award received by the pass-through entity.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="First50k" type="Currency">
            <xs:annotation>
              <xs:documentation>The first $50,000. It is only applicable to one year regardless of the period of performance.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Excess50k" type="Currency">
            <xs:annotation>
              <xs:documentation>The amount paid after the first $50,000.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-LocalUnusedLeavePayment">
    <xs:annotation>
      <xs:documentation>UnusedLeavePayments: Any payment made directly to the employee when the employee retires or terminates employment. An example of this would be unused vacation time paid to the employee after the employee has left the school corporation.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="LocalAccountReference" type="LocalAccountReferenceType">
            <xs:annotation>
              <xs:documentation>References the local account with which the local unused leave payment is associated.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AsOfDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The date of the reported amount for the account.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="RecordIdentifier" type="EXTENSION-RecordIdentifier">
            <xs:annotation>
              <xs:documentation>A unique number or alphanumeric code that identifies the Record.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FinancialCollection" type="FinancialCollectionDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>The accounting period or grouping for which the amount is collected.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>FinancialCollectionDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="EmployeeName" type="FullName">
            <xs:annotation>
              <xs:documentation>The name of employee.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="JobTitle" type="PositionTitle" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The Job title of the employee.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="IndirectUnusedLeavePaymentAmount" type="Currency">
            <xs:annotation>
              <xs:documentation>Payment made to an employee that falls under indirect, incurred for a common or joint purpose benefitting more than one cost objective, and not readily assignable to the cost objectives specifically benefitted, without effort disproportionate to the results achieved.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DirectUnusedLeavePaymentAmount" type="Currency">
            <xs:annotation>
              <xs:documentation>Payment made to an employee that falls under direct costs that can be identified specifically with a particular final cost objective, such as a Federal award, or other internally or externally funded activity, or that can be directly assigned to such activities relatively easily with a high degree of accuracy.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PaymentDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The date the payment was made to the employee.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-SchoolExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="School">
        <xs:sequence>
          <xs:element name="ChoiceIndicator" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicator of whether or not the school is a Choice School.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Accreditation" type="EXTENSION-AccreditationDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>School accreditation (e.g. Accreditted, Not State Accreditted, Recognized Program).</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-AccreditationDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="FreewayBeginDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Freeway accreditation begin date.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FreewayEndDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Freeway accreditation end date.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-SectionDimension">
    <xs:annotation>
      <xs:documentation>This IDOE custom dimension entity represents the valid set of section codes defined by IDOE for a fiscal year reporting period.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="Code" type="Code">
            <xs:annotation>
              <xs:documentation>The code representation of the account section dimension.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FiscalYear" type="LocalFiscalYear">
            <xs:annotation>
              <xs:documentation>The fiscal year for which the account section dimension is valid.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CodeName" type="CodeName" minOccurs="0">
            <xs:annotation>
              <xs:documentation>A description of the account section dimension.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentAccommodation">
    <xs:annotation>
      <xs:documentation>This entity represents the variations used in how an assessment is presented or taken.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="StudentReference" type="StudentReferenceType">
            <xs:annotation>
              <xs:documentation>A unique alphanumeric code assigned to a student.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EducationOrganizationReference" type="EducationOrganizationReferenceType">
            <xs:annotation>
              <xs:documentation>The identifier assigned to an education organization.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AssessmentAccommodationReference" type="EXTENSION-AssessmentAccommodationReferenceType">
            <xs:annotation>
              <xs:documentation>The assessment accommodation reference.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentAssessmentRoster">
    <xs:annotation>
      <xs:documentation>Identifies a student who is expected to participate in particular assessment.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="AssessmentAdministrationReference" type="AssessmentAdministrationReferenceType">
            <xs:annotation>
              <xs:documentation>A reference to the expected administration of an assessment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentReference" type="StudentReferenceType">
            <xs:annotation>
              <xs:documentation>A reference to the student taking the assessment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EducationOrganizationReference" type="EducationOrganizationReferenceType">
            <xs:annotation>
              <xs:documentation>A reference to the student's education organization.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SchoolReference" type="SchoolReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>A reference to the student's enrolled school.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TestingEducationOrganizationReference" type="EducationOrganizationReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>A reference to the education organization expected to administer the assessment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReportingEducationOrganizationReference" type="EducationOrganizationReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>A reference to the education organization which should receive the reults of the assessment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PlatformType" type="PlatformTypeDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The environment or format in which the assessment is expected to be administred</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>PlatformTypeDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="AssessmentCustomization" type="AssessmentCustomization" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Key/value pairs which may be used to facilitate customization of an assessment or to support vendor reporting/analysis</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FirstName" type="FirstName">
            <xs:annotation>
              <xs:documentation>A name given to an individual at birth, baptism, or during another naming ceremony, or through legal change.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MiddleName" type="MiddleName" minOccurs="0">
            <xs:annotation>
              <xs:documentation>A secondary name given to an individual at birth, baptism, or during another naming ceremony.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LastSurname" type="LastSurname">
            <xs:annotation>
              <xs:documentation>The name borne in common by members of a family.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="GenerationCodeSuffix" type="GenerationCodeSuffix" minOccurs="0">
            <xs:annotation>
              <xs:documentation>An appendage, if any, used to denote an individual's generation in his family (e.g., Jr., Sr., III).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BirthDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The month, day, and year on which an individual was born.</xs:documentation>
