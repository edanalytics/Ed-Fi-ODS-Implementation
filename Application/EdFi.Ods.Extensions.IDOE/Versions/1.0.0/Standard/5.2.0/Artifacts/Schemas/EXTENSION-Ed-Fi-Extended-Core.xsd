      <xs:element name="InstructionalMinutes" type="EXTENSION-InstructionalMinutes" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The number of instructional days for the student at the education organization during the course of one full day. Allowable values are 1 - 400.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MembershipCategory" type="EXTENSION-MembershipCategory" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Type of student membership being recorded (e.g. Dual Enrollment, Membership for Adults).</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
