  <xs:simpleType name="EXTENSION-FlexAttendExcessMinutesInInstrSetting">
    <xs:annotation>
      <xs:documentation>E1052. NumberDaysTaught indicates the number of days of instruction offered, as shown on the school calendar, during a particular reporting period.</xs:documentation>
