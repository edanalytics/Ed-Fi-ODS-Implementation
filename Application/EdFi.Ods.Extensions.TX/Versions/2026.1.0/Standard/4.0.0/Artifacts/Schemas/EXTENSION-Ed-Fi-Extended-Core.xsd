<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://ed-fi.org/4.0.0" xmlns:ann="http://ed-fi.org/annotation" targetNamespace="http://ed-fi.org/4.0.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:include schemaLocation="Ed-Fi-Core.xsd" />
  <xs:annotation>
    <xs:documentation>===== Ed-Fi 4.0.0 Extensions =====</xs:documentation>
  </xs:annotation>
  <xs:annotation>
    <xs:documentation>===== Domain Entities =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-AssessmentExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Assessment">
        <xs:sequence>
          <xs:element name="TitleOfAssessment" type="EXTENSION-TitleOfAssessmentDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E1127, C312. The title or name of the assessment.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-TitleOfAssessmentDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReportAssessmentType" type="EXTENSION-ReportAssessmentTypeDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E1573, C306. ReportAssessmentType indicates the TEA collection for which the assessment should be reported.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-ReportAssessmentTypeDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-BasicReportingPeriodAttendance">
    <xs:annotation>
      <xs:documentation>This extended entity represents the recording of a student's regular (basic) reporting period attendance.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="SchoolReference" type="SchoolReferenceType">
            <xs:annotation>
              <xs:documentation>School reference.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentReference" type="StudentReferenceType">
            <xs:annotation>
              <xs:documentation>Student reference.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CalendarCode" type="EXTENSION-CalendarCode">
            <xs:annotation>
              <xs:documentation>E0975. CalendarCode indicates a unique campus calendar of school days for a particular group of students on a campus. Students with the same instructional track have the same number of days taught in a particular reporting period at a campus.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="GradeLevel" type="GradeLevelDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>E0017, C050. GradeLevel indicates the current grade level of the student.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>GradeLevelDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="NumberDaysTaught" type="EXTENSION-NumberDaysTaught">
            <xs:annotation>
              <xs:documentation>E0935. NumberDaysTaught indicates the number of days of instruction offered, as shown on the school calendar, during a particular reporting period.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TotalDaysAbsent" type="EXTENSION-TotalDaysAbsent">
            <xs:annotation>
              <xs:documentation>E0036. TotalDaysAbsent indicates the total number of days the student was absent during a particular reporting period.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TotalIneligibleDaysPresent" type="EXTENSION-TotalIneligibleDaysPresent" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E0936. TotalIneligibleDaysPresent indicates the total number of days the student was present and ineligible for Foundation School Program funding during a particular reporting period.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TotalEligibleDaysPresent" type="EXTENSION-TotalEligibleDaysPresent">
            <xs:annotation>
              <xs:documentation>E0937. TotalEligibleDaysPresent indicates the total number of days the student was present and eligible for Foundation School Program funding during a particular reporting period.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReportingPeriod" type="EXTENSION-ReportingPeriodDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>E0934, C130. ReportingPeriod indicates the period for which the attendance data are being reported.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-ReportingPeriodDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="NumberOfHoursInRPEP" type="EXTENSION-NumberOfHoursInRPEP" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E3090. NumberOfHoursInRPEP captures the number of hours during a particular reporting period a student participates in the Rural Pathway Excellence Partnership (R-PEP) program.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-BilingualESLProgramReportingPeriodAttendance">
    <xs:annotation>
      <xs:documentation>This extended entity represents the bilingual/ESL program attendance of a student.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="SchoolReference" type="SchoolReferenceType">
            <xs:annotation>
              <xs:documentation>School reference.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentReference" type="StudentReferenceType">
            <xs:annotation>
              <xs:documentation>Student reference.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CalendarCode" type="EXTENSION-CalendarCode">
            <xs:annotation>
              <xs:documentation>E0975. CalendarCode indicates a unique campus calendar of school days for a particular group of students on a campus. Students with the same instructional track have the same number of days taught in a particular reporting period at a campus.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="GradeLevel" type="GradeLevelDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>E0017, C050. GradeLevel indicates the current grade level of the student.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>GradeLevelDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="NumberDaysTaught" type="EXTENSION-NumberDaysTaught">
            <xs:annotation>
              <xs:documentation>E0935. NumberDaysTaught indicates the number of days of instruction offered, as shown on the school calendar, during a particular reporting period.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BilingualESLFunding" type="EXTENSION-BilingualESLFundingDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>E1651, C225. BilingualESLFunding indicates the language program in which the student participates during the reporting period.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-BilingualESLFundingDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="TotalEligBilingualESLDaysPresent" type="EXTENSION-TotalEligBilingualESLDaysPresent" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E0938. TotalEligBilingualESLDaysPresent indicates the total number of days the student was present and an eligible participant in the state-approved bilingual/ESL program during a particular reporting period.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReportingPeriod" type="EXTENSION-ReportingPeriodDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>E0934, C130. ReportingPeriod indicates the period for which the attendance data are being reported.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-ReportingPeriodDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-BudgetExt">
    <xs:annotation>
      <xs:documentation>The BudgetExtension Domain Entity represents the amount of monies allocated to be spent or received by an education organization as related to a specific account</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="EducationOrganizationReference" type="EducationOrganizationReferenceType">
            <xs:annotation>
              <xs:documentation>E0212. Reference to a specific LocalEducationAgency</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FiscalYear" type="EXTENSION-FiscalYear">
            <xs:annotation>
              <xs:documentation>E0974. FiscalYear is the last digit of the fiscal year, e.g., the fiscal year is 6 for the 2015-16 fiscal year.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BudgetAmount" type="xs:long">
            <xs:annotation>
              <xs:documentation>E0321. BudgetAmount indicates the dollar value associated with budget financial account information.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BudgetFund" type="EXTENSION-BudgetFundDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>E0316B, C145B. BudgetFund identifies the fund group and specific fund (when applicable) for the budget financial data.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-BudgetFundDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="BudgetFunction" type="EXTENSION-BudgetFunctionDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>E0317, C146. Function identifies a general operational area and groups together related activities.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-BudgetFunctionDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="BudgetObject" type="EXTENSION-BudgetObjectDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>E0318, C159. Object identifies an account, a transaction, or a source of funds.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-BudgetObjectDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="Organization" type="EXTENSION-Organization">
            <xs:annotation>
              <xs:documentation>E0319. Organization identifies the unique organizational unit within the district with which the account is associated.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BudgetProgramIntent" type="EXTENSION-BudgetProgramIntentDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>E0320, C147. ProgramIntent identifies the cost of instruction and other services that are directed toward a particular need of a specific set of students.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-BudgetProgramIntentDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="BeginDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>E3010. BeginDate indicates the month, day, and year for the specified data element becomes true of the student, staff, program, etc.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EndDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E3020. EndDate indicates the month, day, and year for the specified data element becomes false of the student, staff, program, etc.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CalendarDateExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="CalendarDate">
        <xs:sequence>
          <xs:element name="CalendarWaiverEventType" type="EXTENSION-CalendarWaiverEventTypeDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E1570, C204. CalendarWaiverEventType indicates the type of state approved waiver</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-CalendarWaiverEventTypeDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="SchoolDayOperationalMinutes" type="EXTENSION-SchoolDayOperationalMinutes" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E1571. SchoolDayOperationalMinutes indicates the minutes that a campus operated on a particular school day for the purpose of student instruction.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SchoolDayInstructionalMinutes" type="EXTENSION-SchoolDayInstructionalMinutes" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E1599. SchoolDayInstructionalMinutes indicates the portion of the school day in which instruction takes place along with certain other exceptions and are considered a subset of operational minutes.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SchoolDayWaiverMinutes" type="EXTENSION-SchoolDayWaiverMinutes" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E1572. SchoolDayWaiverMinutes indicates the waiver minutes for the campus and calendar date.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-ContractedInstructionalStaffFTEExt">
    <xs:annotation>
      <xs:documentation>The ContractedInstructionalStaffFTEExtension Domain Entity represents financial information for contracted staff by Program Intent Code at the LEA/Campus level.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="EducationOrganizationReference" type="EducationOrganizationReferenceType">
            <xs:annotation>
              <xs:documentation>E0212. Reference to a specific LocalEducationAgency</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SchoolReference" type="SchoolReferenceType">
            <xs:annotation>
              <xs:documentation>E0266. The School denotes the campus with which the contracted staff is associated.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CIStaffProgramIntent" type="EXTENSION-CIStaffProgramIntentDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>E0320, C147. ProgramIntent identifies the cost of instruction and other services that are directed toward a particular need of a specific set of students.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-CIStaffProgramIntentDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="TotalContractedInstrStaffFTE" type="EXTENSION-TotalContractedInstrStaffFTE">
            <xs:annotation>
              <xs:documentation>E0980. TotalContractedInstrStaffFTE is a standard measurement of the portion of professional contracted instructional staff, expressed as a multiple of the standard work day, such as 7.7 FTEs.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CourseTranscriptExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="CourseTranscript">
        <xs:sequence>
          <xs:element name="CollegeCreditHours" type="EXTENSION-CollegeCreditHours" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E1081. CollegeCreditHours indicate the number of college hours a student earned for the completion of a dual credit course.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DualCreditIndicator" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E1011. DualCreditIndicator indicates whether the student was eligible to receive both high school and college credit for a college course.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SummerSchoolIndicator" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E3091. SummerSchoolIndicator indicates the course completed by a student was completed during the summer (between school years).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DropoutRecoveryCourseCompletion" type="EXTENSION-DropoutRecoveryCourseCompletionDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E3097, C355. DropoutRecoveryCourseCompletion indicates if the course completed by a student in a dropout recovery education program under TEC §29.081 was completed in person or remotely.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-DropoutRecoveryCourseCompletionDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="OnRampsDualEnrollmentIndicator" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E3120. OnRampsDualEnrollmentIndicator indicates that the student has completed an OnRamps dual enrollment course.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CTEProgramReportingPeriodAttendance">
    <xs:annotation>
      <xs:documentation>This extended entity represents the CTE program attendance of a student.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="SchoolReference" type="SchoolReferenceType">
            <xs:annotation>
              <xs:documentation>School reference.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentReference" type="StudentReferenceType">
            <xs:annotation>
              <xs:documentation>Student reference.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CalendarCode" type="EXTENSION-CalendarCode">
            <xs:annotation>
              <xs:documentation>E0975. CalendarCode indicates a unique campus calendar of school days for a particular group of students on a campus. Students with the same instructional track have the same number of days taught in a particular reporting period at a campus.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="GradeLevel" type="GradeLevelDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>E0017, C050. GradeLevel indicates the current grade level of the student.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>GradeLevelDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="NumberDaysTaught" type="EXTENSION-NumberDaysTaught">
            <xs:annotation>
              <xs:documentation>E0935. NumberDaysTaught indicates the number of days of instruction offered, as shown on the school calendar, during a particular reporting period.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CTEServiceId" type="EXTENSION-CTEServiceIdDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>E3013, C330. CTEServiceID refers to the approved career and technical education course that generates countact hours during a particular reporting period.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-CTEServiceIdDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="EligibleDaysPresentV1" type="EXTENSION-EligibleDaysPresentV1" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E0950. EligibleDaysPresentV1 indicates the total number of days the student was eligible and present and enrolled in an approved career and technical course that generated one contact hour during a particular reporting period.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EligibleDaysPresentV2" type="EXTENSION-EligibleDaysPresentV2" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E0951. EligibleDaysPresentV2 indicates the total number of days the student was eligible and present and enrolled in an approved career and technical course that generated two contact hours during a particular reporting period.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EligibleDaysPresentV3" type="EXTENSION-EligibleDaysPresentV3" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E0952. EligibleDaysPresentV3 indicates the total number of days the student was eligible and present and enrolled in an approved career and technical course that generated three contact hours during a particular reporting period.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReportingPeriod" type="EXTENSION-ReportingPeriodDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>E0934, C130. ReportingPeriod indicates the period for which the attendance data are being reported.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-ReportingPeriodDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-DescriptorMappingHistory">
    <xs:annotation>
      <xs:documentation>This entity is intended to log all changes to the descriptormapping table</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="Value" type="EXTENSION-Value">
            <xs:annotation>
              <xs:documentation>The descriptor value that is being mapped to another value.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Namespace" type="URI">
            <xs:annotation>
              <xs:documentation>The namespace of the descriptor value that is being mapped to another value.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MappedValue" type="EXTENSION-MappedValue">
            <xs:annotation>
              <xs:documentation>The descriptor value to which the from descriptor value is being mapped to.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MappedNamespace" type="URI">
            <xs:annotation>
              <xs:documentation>The namespace of the descriptor value to which the from descriptor value is mapped to.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DateOfOperation" type="xs:dateTime">
            <xs:annotation>
              <xs:documentation>The date/time when the operation occurred</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DescriptorMappingId" type="EXTENSION-DescriptorMappingId">
            <xs:annotation>
              <xs:documentation>The id of the resource in question</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EducationOrganizationReference" type="EducationOrganizationReferenceType">
            <xs:annotation>
              <xs:documentation>A reference to the the organization which created/modified the mapping</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SchoolYear" type="SchoolYearType">
            <xs:annotation>
              <xs:documentation>School year in which the mapping was created/modified</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ApiOperationType" type="EXTENSION-ApiOperationTypeDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>The type of operation performed on the descriptor</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-ApiOperationTypeDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="AccountName" type="AccountName" minOccurs="0">
            <xs:annotation>
              <xs:documentation>User name of the account which created/modified the mapping</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-DisciplineActionExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DisciplineAction">
        <xs:sequence>
          <xs:element name="ActualLengthOfDisciplinaryAssignment" type="EXTENSION-ActualLengthOfDisciplinaryAssignment" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E1008. ActualLengthOfDisciplinaryAssignment indicates the actual length in days of a student’s disciplinary assignment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="OfficialLengthOfDisciplinaryAssignment" type="EXTENSION-OfficialLengthOfDisciplinaryAssignment" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E1007. OfficialLengthOfDisciplinaryAssignment indicates the official length in days of a student’s disciplinary assignment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="InconsistentCodeOfConduct" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>E1656. InconsistentCodeOfConduct indicates whether an out-of-school suspension, disciplinary alternative education program placement, or expulsion action is inconsistent with a local education agency’s student code of conduct.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="NonMembershipDisciplineRestraintIndicator" type="xs:boolean" minOccurs="0">
            <xs:annotation>
